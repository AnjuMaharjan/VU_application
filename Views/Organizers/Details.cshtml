@model VU.Models.Organizer

@{
    ViewBag.Title = "Details";
}

<h3>Organizer Details</h3>

<div>
    <h4>Organizer</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Organizer Name")
            @*@Html.DisplayNameFor(model => model.OrganizerName)*@
        </dt>

        <dd>
            @Html.DisplayFor(model => model.OrganizerName)
        </dd>

        <dt>
            @Html.DisplayName("Contact Person")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ContactPerson)
        </dd>

        <dt>
            @Html.DisplayName("Email")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.OrganizerEmail)
        </dd>

        <dt>
            @Html.DisplayName("Phone")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.OrganizerPhone)
        </dd>

        <dt>
            @Html.DisplayName("Address")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.OrganizerAddress)
        </dd>

    </dl>
</div>
<p>
    
    @Html.ActionLink("Edit", "Edit", new { id = Model.OrganizerID }) |
    @Html.ActionLink("Back to List", "Index")
